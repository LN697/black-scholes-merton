{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/include"
            ],
            "defines": [
                "__MINGW32__",
                "__MINGW64__",
                "_GLIBCXX_USE_FLOAT128=0",
                "__NO_INLINE__",
                "__STRICT_ANSI__",
                "_GNU_SOURCE=0",
                "__NO_MATH_INLINES",
                "__GLIBC_USE_FLOAT128=0",
                "_GLIBCXX_USE_C99_STDINT_TR1=1",
                "__STDC_LIMIT_MACROS",
                "__STDC_CONSTANT_MACROS"
            ],
            "compilerPath": "C:/mingw64/bin/x86_64-w64-mingw32-g++.exe",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x64",
            "compilerArgs": [
                "-std=c++17",
                "-D_GLIBCXX_USE_FLOAT128=0",
                "-U__STRICT_ANSI__",
                "-fno-builtin"
            ],
            "compileCommands": "${workspaceFolder}/compile_commands.json",
            "browse": {
                "path": [
                    "${workspaceFolder}/include",
                    "${workspaceFolder}/src"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}
